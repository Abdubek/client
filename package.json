{
  "name": "logux-client",
  "version": "0.2.10",
  "description": "Low-level Logux client for web browser",
  "keywords": [
    "logux",
    "client"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/logux-client",
  "dependencies": {
    "logux-core": "logux/logux-core",
    "nanoevents": "^1.0.4",
    "nanoid": "^1.0.4"
  },
  "devDependencies": {
    "docdash": "^0.4.0",
    "eslint": "^5.0.1",
    "eslint-config-logux": "^23.0.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.1.0",
    "fake-indexeddb": "^2.0.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.3.0",
    "jsdoc": "^3.5.5",
    "lint-staged": "^7.2.0",
    "nanodelay": "^0.1.3",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "size-limit": "^0.18.3",
    "webpack": "^4.15.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "yaspeller-ci": "^1.0.0"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "spellcheck": "yarn docs && yaspeller-ci *.md docs/*.html",
    "clean": "rimraf docs/ coverage/ test/demo/build/",
    "lint": "eslint *.js test/{**/,}*.js",
    "docs": "jsdoc --configure .jsdocrc *.js",
    "test": "jest --coverage && yarn lint && size-limit && yarn spellcheck",
    "start": "webpack-dev-server --config test/demo/webpack.config"
  },
  "pre-commit": [
    "lint-staged"
  ]
}
